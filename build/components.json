[
    {
        "tag": "Button",
        "description": "A Button",
        "variants": ["black", "blue"],
        "props": {
            "variant": {
                "type": "string",
                "description": "Button color variant",
                "enum": ["black", "blue"],
                "default": "black"
            },
            "children": {
                "type": "string",
                "description": "Button text",
                "required": true
            },
            "onClick": {
                "type": "function",
                "description": "Click handler"
            }
        },
        "examples": [
            {
                "description": "Black button",
                "code": "<Button variant=\"black\" onClick={handleClick}>Click me</Button>"
            },
            {
                "description": "Blue button",
                "code": "<Button variant=\"blue\" onClick={handleSubmit}>Submit</Button>"
            }
        ]
    }
]
